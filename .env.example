# AI Provider (auto | gemini | doubao | openai)
NEXT_PUBLIC_AI_PROVIDER=auto

# Doubao AI (chat + vision for analyze)
NEXT_DOUBAO_API_KEY=your_doubao_key
NEXT_DOUBAO_CHAT_MODEL=doubao-seed-1-6-lite-251015
NEXT_DOUBAO_CHAT_ENDPOINT=https://ark.cn-beijing.volces.com/api/v3/chat/completions

# Doubao Image Generation (for image model "Doubao" in UI)
DOUBAO_IMAGE_ENDPOINT=https://ark.cn-beijing.volces.com/api/v3/images/generations
DOUBAO_IMAGE_MODEL=doubao-seedream-4-0-250828

# Google Gemini (required for image/voice in Cinematic Sketch)
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_key
NEXT_PUBLIC_GEMINI_MODEL=gemini-2.5-flash

# OpenAI
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_key
NEXT_PUBLIC_OPENAI_MODEL=gpt-4o-mini

# Supabase (optional)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
